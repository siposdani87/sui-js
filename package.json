{
  "name": "@siposdani87/sui-js",
  "version": "0.7.0",
  "description": "Powerfull Vanilla JavaScript framework for web and react(-native)",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean && npm run sass-lint && npm run eslint && npm run format",
    "build": "npm run sass && npm run tsc && npm run esbuild",
    "clean": "rm -rf dist",
    "tsc": "npx -p typescript tsc",
    "sass": "npx sass styles/sui.scss dist/sui.min.css --style compressed",
    "esbuild": "npx esbuild src/index.ts --bundle --minify --sourcemap --define:ES_PROD=true --format=iife --global-name=SUI --outfile=dist/sui.min.js",
    "eslint": "npx eslint . --ext .ts,.tsx",
    "sass-lint": "npx sass-lint --verbose --no-exit",
    "format": "npx prettier --write 'src/**/*.ts*'"
  },
  "license": "UNLICENSED",
  "private": true,
  "keywords": [
    "sui-js"
  ],
  "author": "DaÃÅniel Sipos <siposdani87@gmail.com> (https://siposdani87.com)",
  "homepage": "https://siposdani87.com",
  "files": [
    "dist/*",
    "src/*",
    "styles/*"
  ],
  "dependencies": {
    "@rails/actioncable": "6.1.5",
    "crypto-js": "4.1.1",
    "date-fns": "2.28.0",
