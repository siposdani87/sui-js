    "clean-jsdoc-theme": "3.3.4",
    "connect-modrewrite": "0.10.2",
    "esbuild": "0.14.28",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "38.1.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
