{
  "name": "@siposdani87/sui-js",
  "version": "0.6.0",
  "description": "Powerfull Vanilla JavaScript framework for web and react(-native), based on Google Closure Compiler",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "tsc-test": "npx -p typescript tsc --noEmit",
    "precompile": "npm run clean",
    "compile": "npx -p typescript tsc",
    "prebuild": "npm run compile",
    "build": "gulp",
    "preesbuild": "npm run compile && gulp compile:styles",
    "esbuild": "npx esbuild src/index.ts --bundle --minify --sourcemap --define:ES_PROD=false --format=iife --global-name=SUI --outfile=dist/sui.min.js",
    "clean": "rm -rf dist",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "npx prettier --write 'src/**/*.ts*'"
  },
  "license": "UNLICENSED",
  "private": true,
  "keywords": [
    "sui-js"
  ],
  "author": "DaÃÅniel Sipos <siposdani87@gmail.com> (https://siposdani87.com)",
  "homepage": "https://siposdani87.com",
  "files": [
    "dist/*",
    "src/*",
    "styles/*"
  ],
  "dependencies": {
    "@rails/actioncable": "6.1.5",
    "crypto-js": "4.1.1",
    "date-fns": "2.28.0",
    "material-design-lite": "1.3.0",
    "moment": "2.29.1",
    "normalize.css": "8.0.1"
  },
  "devDependencies": {
    "@types/google.maps": "3.48.2",
    "@types/material-design-lite": "^1.1.17",
    "@types/rails__actioncable": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "browser-sync": "2.27.9",
    "clean-jsdoc-theme": "3.3.4",
    "connect-modrewrite": "0.10.2",
    "esbuild": "^0.14.25",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "38.0.4",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "google-closure-compiler": "20220301.0.0",
    "google-closure-library": "20220301.0.0",
    "gulp": "4.0.2",
    "gulp-cli": "2.3.0",
    "gulp-insert": "0.5.0",
