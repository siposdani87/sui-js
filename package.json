    "@types/rails__actioncable": "6.1.6",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "browser-sync": "2.27.9",
    "clean-jsdoc-theme": "3.3.4",
    "connect-modrewrite": "0.10.2",
    "esbuild": "0.14.36",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
