{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2015",
    "moduleResolution": "Node",
    "declaration": true,
    "outDir": "dist",
    "types": ["google.maps", "jest"],
    "lib": ["ESNext", "DOM"],
    // "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules",
    "**/*.test.ts"
  ]
}
